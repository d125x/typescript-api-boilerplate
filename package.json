{
  "name": "typescript-api-boilerplate",
  "version": "1.0.0",
  "description": "REST API server boilerplate written in Node.js and Typescript",
  "engines": {
    "node": ">=12.0.0",
    "npm": ">=6.0.0"
  },
  "homepage": "https://github.com/sang-uk-lee/typescript-api-boilerplate#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sang-uk-lee/typescript-api-boilerplate"
  },
  "bugs": {
    "url": "https://github.com/sang-uk-lee/typescript-api-boilerplate/issues",
    "email": "lee@sang-uk.kr"
  },
  "author": {
    "name": "Sang-Uk Lee (이상욱)",
    "email": "lee@sang-uk.kr",
    "url": "https://sang-uk.kr"
  },
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "start:l": "cross-env NODE_ENV=local yarn dev",
    "start:d": "cross-env NODE_ENV=development yarn dev",
    "start:p": "cross-env NODE_ENV=production yarn dev",
    "clean": "rimraf build",
    "lint": "eslint src/**/*.ts",
    "test": "jest --passWithNoTests --coverage",
    "prebuild": "yarn --production && yarn clean",
    "build": "webpack --color --progress profile",
    "postbuild": "yarn test",
    "pm2": "pm2 start && pm2 monit",
    "deploy": "yarn build --node-env=production && yarn pm2",
    "kill": "pm2 kill && fkill -f -v node.exe"
  },
  "dependencies": {
    "apicache": "^1.6.2",
    "app-root-path": "^3.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "express-brute": "^1.0.1",
    "express-validator": "^6.11.1",
    "fs": "0.0.1-security",
    "helmet": "^4.6.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "moment-timezone": "^0.5.33",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "mysql2-promise": "^0.1.4",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "sqlstring": "^2.3.2",
    "toobusy-js": "^0.5.1",
    "typedi": "^0.9.1",
    "uuid": "^8.3.2",
    "validator": "^13.6.0",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-typescript": "^7.13.0",
    "@types/apicache": "^1.2.2",
    "@types/app-root-path": "^1.2.4",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.10",
    "@types/crypto-js": "^3.1.47",
    "@types/express": "^4.17.12",
    "@types/express-brute": "^1.0.0",
    "@types/helmet": "^0.0.48",
    "@types/hpp": "^0.2.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.168",
    "@types/morgan": "^1.9.2",
    "@types/mysql2": "types/mysql2#",
    "@types/node": "^14.14.44",
    "@types/node-rsa": "^1.1.0",
    "@types/passport": "^1.0.6",
    "@types/passport-jwt": "^3.0.5",
    "@types/passport-local": "^1.0.33",
    "@types/sqlstring": "^2.3.0",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/terser-webpack-plugin": "^5.0.3",
    "@types/toobusy-js": "^0.5.2",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.4.1",
    "@typescript-eslint/parser": "^4.5.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.14.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sort-class-members": "^1.9.0",
    "fkill-cli": "^6.1.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "pm2": "^4.5.6",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "swagger-jsdoc": "6.0.0",
    "swagger-ui-express": "^4.1.6",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.3",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0",
    "webpack-node-externals": "^3.0.0",
    "webpack-obfuscator": "^3.3.0"
  }
}
